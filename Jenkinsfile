pipeline {
  agent any
  stages {
    stage('checkout') {
      steps {
        echo ('checkout the pipeline')
      }
    }
    stage('build') {
      steps {
        echo ('build the pipeline')
      }
    }
    stage('deploy') {
      steps {
        echo ('deploy the pipeline')
      }
    }
    stage('testing') {
      steps {
        echo ('test the pipeline')
      }
    }
  }
}
